{
  "name": "vue-emitting-analog-clock",
  "version": "1.0.0",
  "description": "Emitting analog clock component for vuejs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moritanian/vue-emitting-analog-clock.git"
  },
  "homepage": "https://github.com/moritanian/vue-emitting-analog-clock",
  "main": "dist/vue-emitting-analog-clock.umd.js",
  "module": "dist/vue-emitting-analog-clock.esm.js",
  "unpkg": "dist/vue-emitting-analog-clock.min.js",
  "browser": {
    "./sfc": "src/vue-emitting-analog-clock.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "keywords": [
    "clock",
    "js",
    "vue",
    "analog clock",
    "emitting analog clock"
  ],
  "author": "Moritanian <pickit2express@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "emitting-analog-clock": "^1.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  }
}
